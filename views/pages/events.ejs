<div class="jumbotron text-center">
  <h1>All Events</h1>
</div>

<table class="table table-bordered table-hover table-striped">
  <thread>
    <tr>
      <th>Event Name</th>
      <th>Description</th>
      <th></th>
    </thread>
    </tr>
  <tbody>

      <% for (var event of events) { %>
        <tr>
          <td><%= event.name %></td>
          <td><%= event.description %></td>
          <td>
            <a href="/events/<% event.slug %>" class="btn btn-sm btn-primary">View</a>
          </td>
        </tr>
      <% } %>
  </tbody>
</table>
